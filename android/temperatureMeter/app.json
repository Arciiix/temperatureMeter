{
  "expo": {
    "name": "Temperatura",
    "slug": "temperatureMeter",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["android"],
    "androidStatusBar": {
      "backgroundColor": "#3ead57"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3ead57"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "icon": "./assets/trayIcon.png",
      "color": "#3ead57"
    },
    "android": {
      "package": "com.Arciiix.temperatureMeter",
      "googleServicesFile": "./google-services.json",
      "backgroundColor": "#3ead57"
    }
  }
}
